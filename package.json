{
    "name": "homepage",
    "private": true,
    "description": "My Personal Homepage.",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "scripts": {
        "build_debug": "DEPLOY_ORIGIN='https://dev.chris-besch.com' node_modules/.bin/gatsby build",
        "build_deploy": "DEPLOY_ORIGIN='https://chris-besch.com' node_modules/.bin/gatsby build",
        "develop": "DEPLOY_ORIGIN='http://localhost:8000' node_modules/.bin/gatsby develop",
        "serve": "node_modules/.bin/gatsby serve",
        "clean": "node_modules/.bin/gatsby clean",
        "type_check": "find ./src/ -name __generated__ -type d | xargs rm -r -v || true && node_modules/.bin/tsgql typegen && node_modules/.bin/tsc && echo 'type check ok'",
        "type_check:watch": "node_modules/.bin/tsc --watch",
        "load_gql": "node_modules/.bin/get-graphql-schema http://localhost:8000/___graphql > schema.graphql && yarn run type_check"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/christopher-besch/homepage"
    },
    "devDependencies": {
        "@mdx-js/mdx": "v1",
        "@mdx-js/react": "v1",
        "@reach/router": "^1.3.4",
        "@types/node": "^17.0.23",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/react-helmet": "^6.1.5",
        "gatsby": "^4.11.2",
        "gatsby-link": "^4.11.1",
        "gatsby-plugin-image": "^2.11.1",
        "gatsby-plugin-mdx": "^3.11.1",
        "gatsby-plugin-react-helmet": "^5.11.0",
        "gatsby-plugin-sass": "^5.11.1",
        "gatsby-plugin-sharp": "^4.11.1",
        "gatsby-plugin-tsconfig-paths": "^1.0.5",
        "gatsby-react-router-scroll": "^5.11.0",
        "gatsby-remark-autolink-headers": "^5.11.1",
        "gatsby-remark-images": "^6.11.1",
        "gatsby-remark-table-of-contents": "^2.0.0",
        "gatsby-source-filesystem": "^4.11.1",
        "gatsby-transformer-sharp": "^4.11.0",
        "get-graphql-schema": "^2.1.2",
        "prism-react-renderer": "^1.3.1",
        "query-string": "^7.1.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-helmet": "^6.1.0",
        "sass": "^1.50.0",
        "ts-graphql-plugin": "^2.1.6",
        "types": "^0.1.1",
        "typescript": "^4.6.3",
        "yarn-upgrade-all": "^0.7.1"
    }
}
