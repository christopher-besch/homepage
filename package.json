{
    "name": "homepage",
    "private": true,
    "description": "My Personal Homepage.",
    "version": "0.1.0",
    "license": "UNLICENSED",
    "scripts": {
        "build_debug": "DEPLOY_ORIGIN='https://dev.chris-besch.com' node_modules/.bin/gatsby build",
        "build_deploy": "DEPLOY_ORIGIN='https://chris-besch.com' node_modules/.bin/gatsby build",
        "develop": "DEPLOY_ORIGIN='http://localhost:8000' node_modules/.bin/gatsby develop",
        "serve": "node_modules/.bin/gatsby serve",
        "clean": "node_modules/.bin/gatsby clean",
        "type-check": "find ./src/ -name __generated__ -type d | xargs rm -r -v || true && node_modules/.bin/tsgql typegen && node_modules/.bin/tsc && echo 'type check ok'",
        "type-check:watch": "node_modules/.bin/tsc --watch",
        "load-gql": "node_modules/.bin/get-graphql-schema http://localhost:8000/___graphql > schema.graphql && yarn run type-check"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/christopher-besch/homepage"
    },
    "devDependencies": {
        "@mdx-js/mdx": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "@reach/router": "^1.3.4",
        "@types/node": "^16.11.11",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.5",
        "gatsby": "^4.3.0",
        "gatsby-link": "^4.3.0",
        "gatsby-plugin-image": "^2.3.0",
        "gatsby-plugin-mdx": "^3.6.0",
        "gatsby-plugin-react-helmet": "^5.3.0",
        "gatsby-plugin-sass": "^5.3.0",
        "gatsby-plugin-sharp": "^4.3.0",
        "gatsby-plugin-tsconfig-paths": "^1.0.4",
        "gatsby-react-router-scroll": "^5.3.0",
        "gatsby-remark-autolink-headers": "^5.6.0",
        "gatsby-remark-images": "^6.6.0",
        "gatsby-remark-table-of-contents": "^2.0.0",
        "gatsby-source-filesystem": "^4.3.0",
        "gatsby-transformer-sharp": "^4.3.0",
        "get-graphql-schema": "^2.1.2",
        "prism-react-renderer": "^1.2.1",
        "query-string": "^7.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "sass": "^1.44.0",
        "ts-graphql-plugin": "^2.1.5",
        "types": "^0.1.1",
        "typescript": "^4.5.2"
    }
}
